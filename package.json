{
  "name": "mms-frontend",
  "version": "1.0.0",
  "description": "The react frontend for the membership management system",
  "main": "index.jsx",
  "scripts": {
    "build": "webpack --progress --profile --colors",
    "start": "npm run serve | npm run dev",
    "serve": "./node_modules/.bin/http-server -p 8080",
    "dev": "webpack-dev-server --hot --progress --colors --port 8090"
  },
  "author": "Dexter",
  "license": "ISC",
  "dest": {
    "app": "app.js",
    "dist": "build",
    "vendor": "build/vendor"
  },
  "paths": {
    "jsx": "./app/**/*.jsx",
    "html": "./app/*.html",
    "less": "./app/*.less",
    "json": "./app/*.json",
    "img": "./app/img/*"
  },
  "dependencies": {
    "axios": "^0.9.0",
    "classnames": "^2.2.3",
    "fetch": "^1.0.0",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.0.0",
    "material-design-icons": "^2.1.3",
    "material-design-lite": "^1.0.6",
    "material-ui": "^0.14.2",
    "react": "^0.14.6",
    "react-addons": "^0.9.0",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dnd-touch-backend": "^0.2.4",
    "react-dom": "^0.14.6",
    "react-mdl": "^1.2.0",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "react-tap-event-plugin": "^0.2.1",
    "react-ui-datepicker": "^0.1.0",
    "redux": "^3.0.5",
    "redux-localstorage": "*",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^1.0.3",
    "resolve-url-loader": "^1.4.3"
  },
  "devDependencies": {
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "babel-preset-stage-1": "*",
    "css-loader": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "font-loader": "*",
    "html-webpack-plugin": "*",
    "http-server": "*",
    "json-loader": "*",
    "jsx-loader": "*",
    "less": "*",
    "less-loader": "*",
    "react-hot-loader": "*",
    "redux-devtools": "*",
    "redux-devtools-dock-monitor": "*",
    "redux-devtools-log-monitor": "*",
    "redux-logger": "*",
    "serviceworker-loader": "*",
    "style-loader": "*",
    "url-loader": "*",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
