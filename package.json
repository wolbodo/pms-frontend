{
  "name": "mms-frontend",
  "version": "1.0.0",
  "description": "The react frontend for the membership management system",
  "main": "index.jsx",
  "scripts": {
    "build": "webpack --progress --profile --colors",
    "start": "npm run serve | npm run dev",
    "serve": "./node_modules/.bin/http-server -p 8080",
    "dev": "webpack-dev-server --hot --progress --colors --port 8090"
  },
  "author": "Dexter",
  "license": "ISC",
  "dest": {
    "app": "app.js",
    "dist": "build",
    "vendor": "build/vendor"
  },
  "paths": {
    "jsx": "./app/**/*.jsx",
    "html": "./app/*.html",
    "less": "./app/*.less",
    "json": "./app/*.json",
    "img": "./app/img/*"
  },
  "dependencies": {
    "axios": "^0.7.0",
    "belle": "^2.0.0-rc2",
    "classnames": "^2.2.1",
    "history": "^1.13.0",
    "lodash": "^3.10.1",
    "material-design-icons": "^2.0.0",
    "material-design-lite": "^1.0.5",
    "material-ui": "^0.13.4",
    "react": "^0.14.0",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.0",
    "react-dnd-touch-backend": "^0.2.3",
    "react-dom": "^0.14.0",
    "react-mdl": "^0.15.0",
    "react-router": "^1.0.0-rc3",
    "react-tap-event-plugin": "^0.2.1",
    "react-ui-datepicker": "^0.1.0",
    "resolve-url-loader": "^1.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "font-loader": "^0.1.2",
    "html-webpack-plugin": "^1.7.0",
    "http-server": "^0.8.5",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
